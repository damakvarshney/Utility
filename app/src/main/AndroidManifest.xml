<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rehash.utility">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".screens.sublist.registerScreens.SimpleRegisterActivity"
            android:theme="@style/Theme.Design.NoActionBar"></activity>
        <activity android:name=".screens.sublist.loginScreens.LoginInBoxScreen" />
        <activity android:name=".screens.HomeScreen" />
        <activity android:name=".screens.sublist.loginScreens.SimpleLoginScreen" />
        <activity android:name=".screens.RegisterScreen" />
        <activity android:name=".screens.LoginScreens" />
        <activity android:name=".screens.MainScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>